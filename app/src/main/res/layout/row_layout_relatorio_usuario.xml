<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="16dp"
    android:padding="5dp"
    android:background="@drawable/customedittextbackground"
    android:layout_marginBottom="5dp">

    <TextView
        android:textSize="30sp"
        android:id="@+id/TV_RowLabel_Nome"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="@string/TXLoading"/>
    <TextView
        android:textSize="20sp"
        android:id="@+id/TV_Relatorio_Usuario_Admin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/TXLoading"/>
    <TextView
        android:textSize="20sp"
        android:id="@+id/TV_Relatorio_Usuario_Nome"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/TXLoading"/>
    <TextView
        android:textSize="20sp"
        android:id="@+id/TV_Relatorio_Usuario_Criacao"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/TXLoading"/>
</LinearLayout>